<h1>Listing agents</h1>

<table>
  <tr>
    <th>User</th>
    <th>Agency</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @agents.each do |agent| %>
  <tr>
    <td><%= agent.user_id %></td>
    <td><%= agent.agency_id %></td>
    <td><%= agent.active %></td>
    <td><%= link_to 'Show', agent %></td>
    <td><%= link_to 'Edit', edit_agent_path(agent) %></td>
    <td><%= link_to 'Destroy', agent, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

