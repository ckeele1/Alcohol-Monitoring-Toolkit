<h1>Editing <%= singular_name %></h1>

<%%= form_for(@<%= singular_name %>) do |f| %>
	<%%= render :partial => 'shared/notice_messages', :locals => { :css_id_prefix => '<%= singular_name %>_', :error_messages => f.error_messages } %>

	<%%= render :partial => 'form', :locals => { :f => f } %>
	<div class="actions submit">
		<%%= f.submit %> or <%%= link_to 'cancel', <%= plural_name %>_path %>
	</div>
<%% end %>